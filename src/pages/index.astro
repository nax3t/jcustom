---
import Layout from '../layouts/Layout.astro';
import Nav from '../components/Nav.astro';
import HeroSection from '../components/HeroSection.astro';
import ProfileSection from '../components/ProfileSection.astro';
import ServicesSection from '../components/ServicesSection.astro';
import BannerSection from '../components/BannerSection.astro';
import ShowcaseSection from '../components/ShowcaseSection.astro';
import CTASection from '../components/CTASection.astro';
import TestimonialSection from '../components/TestimonialSection.astro';
import PromoSection from '../components/PromoSection.astro';
import ServiceAreasSection from '../components/ServiceAreasSection.astro';
import NewsletterSection from '../components/NewsletterSection.astro';
import ContactSection from '../components/ContactSection.astro';
import Footer from '../components/Footer.astro';
---

<Layout>
  <Nav />
  <main id="main">
    <HeroSection />
    <ProfileSection />
    <BannerSection />
    <ServicesSection />
    <ShowcaseSection />
    <CTASection />
    <TestimonialSection />
    <PromoSection />
    <ServiceAreasSection />
    <ContactSection />
    <NewsletterSection />
  </main>
  <Footer />
</Layout>
